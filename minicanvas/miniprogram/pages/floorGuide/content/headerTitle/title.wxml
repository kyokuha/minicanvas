<template name="title">
  <view class="floor-title">
    <text>{{floorNum}}层</text>
    <block wx:for="{{floorInfo}}" wx:for-item="singleRoom" wx:for-index="in" wx:key="singleRoom.subfloorNum">
      <text>{{singleRoom.floorName}}</text>
    </block>
    <!-- <text>1层\n视频监控大厅 物业服务 营销服务大厅</text> -->
  </view>
  <!-- <view class="scale-button" bindtap="scaleZoomOut" wx:if="{{onBrowsing}}" > -->
    <view class="scale-button" bindtap="scaleZoomOut" animation="{{anime}}">
    <div style="font-family: serif; font-size: 30rpx;">
      当前为地图导览模式：点击返回手动缩放
    </div>
  </view>
</template>